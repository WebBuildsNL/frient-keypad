      "path": "/codes"
    },
    "deleteCodes": {
      "method": "DELETE",
      "path": "/codes"
    }
  },
  "images": {
    "small": "/assets/images/small.png",
    "large": "/assets/images/large.png"
  },
  "author": {
    "name": "Dennis"
  }
}
